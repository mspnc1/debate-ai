{
  "name": "symposium-ai",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --clean",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "clean": "rm -rf ios android node_modules && npm install",
    "clean:ios": "cd ios && rm -rf Pods Podfile.lock && pod install && cd ..",
    "build:dev:ios": "eas build --platform ios --profile development --local",
    "build:dev:android": "eas build --platform android --profile development --local",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run test",
    "discover:models": "node scripts/api-discovery/discover-all-models.js",
    "docs:providers": "node scripts/api-discovery/discover-all-models.js && echo 'Provider docs refreshed.'",
    "demo:build-recordings": "node scripts/demo/build-recordings-manifest.js",
    "demo:bridge": "node scripts/demo/dev-bridge.mjs",
    "test": "jest --watch=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@expo/metro-runtime": "~5.0.4",
