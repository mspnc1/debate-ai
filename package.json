    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.5",
    "@reduxjs/toolkit": "^2.9.0",
    "axios": "^1.11.0",
    "expo": "54.0.11",
    "expo-apple-authentication": "~8.0.7",
    "expo-audio": "~1.0.13",
    "expo-auth-session": "~7.0.8",
