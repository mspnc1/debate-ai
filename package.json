{
  "name": "symposium-ai",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --clear",
    "prebuild:ios": "expo prebuild --platform ios --clear",
    "prebuild:android": "expo prebuild --platform android --clear",
    "clean": "rm -rf ios android node_modules && npm install",
    "clean:ios": "cd ios && rm -rf Pods Podfile.lock && pod install && cd ..",
    "build:dev:ios": "eas build --platform ios --profile development --local",
    "build:dev:android": "eas build --platform android --profile development --local",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@expo/metro-runtime": "~5.0.4",
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-firebase/app": "^23.1.1",
    "@react-native-firebase/auth": "^23.1.1",
    "@react-native-firebase/firestore": "^23.1.1",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.5",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.11.0",
    "expo": "~53.0.20",
    "expo-blur": "^14.1.5",
    "expo-clipboard": "^7.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-haptics": "^14.1.4",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-print": "~14.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "~2.2.3",
    "openai": "^5.12.2",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-sse": "^1.2.1",
    "react-native-svg": "15.11.2",
    "react-native-view-shot": "^4.0.3",
    "react-native-web": "^0.20.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.33.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "sharp": "^0.34.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0"
  },
  "private": true
}
