    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "sharp": "^0.34.4",
    "typescript": "~5.8.3",
